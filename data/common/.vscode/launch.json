{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "internalConsole"
    },
    {
      "name": "find",
      "type": "python",
      "request": "launch",
      "module": "find",
      "args": [
        "projects/write-dissertation/attend-the-ai-for-thermal-energy-science-conference/conference.md",
        "[-\\s]source\\s"
      ],
      "console": "internalConsole"
    },
    {
      "name": "preview",
      "type": "python",
      "request": "launch",
      "module": "preview",
      "args": ["reports/23-02-01_to_23-05-03.md"],
      "console": "internalConsole"
    },
    {
      "name": "PowerShell Launch Current File",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "cwd": "${cwd}"
    }
  ]
}
